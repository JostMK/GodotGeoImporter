[configuration]
entry_symbol = "example_library_init"
compatibility_minimum = 4.3
reloadable = true

[icons]
GDExample = "icons/Example.svg"

[libraries]
windows.release.x86_64 = "bin/lib${SHORT_PROJECT_NAME}-Windows-AMD64.dll"
